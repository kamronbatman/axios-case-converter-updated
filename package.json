{
  "name": "axios-case-converter-updated",
  "version": "0.4.0",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "lint-src": "eslint src",
    "lint-examples": "eslint examples",
    "test": "npm run lint-src && cross-env BABEL_ENV=cjs babel-node test/index.js | tap-spec",
    "test-without-lint": "cross-env BABEL_ENV=cjs babel-node test/index.js | tap-spec",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build": "run-p build:*",
    "prepare": "install-peers",
    "prepublish": "in-publish && (git diff --no-ext-diff --quiet --exit-code src .gitignore package.json README.md LICENSE || (echo \"Please commit changes!\" && false)) && npm test && npm run build || not-in-publish"
  },
  "description": "Axios transformer/interceptor that converts snake_case/camelCase",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamronbatman/axios-interceptor-change-case.git"
  },
  "keywords": [
    "axios",
    "interceptor",
    "snake",
    "camel",
    "case",
    "convert"
  ],
  "author": "mpyw <ryosuke_i_628@yahoo.co.jp>, Kamron Batman <3953314+kamronbatman@users.noreply.github.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mpyw/axios-interceptor-change-case/issues"
  },
  "homepage": "https://github.com/mpyw/axios-interceptor-change-case#readme",
  "peerDependencies": {
    "axios": "^0.19.2",
    "axios-mock-adapter": "^1.17.0"
  },
  "dependencies": {
    "camel-case": "^4.1.1",
    "header-case": "^2.0.3",
    "snake-case": "^3.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "in-publish": "^2.0.0",
    "install-peers-cli": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "tap-spec": "^5.0.0",
    "tape": "^4.13.0"
  }
}
